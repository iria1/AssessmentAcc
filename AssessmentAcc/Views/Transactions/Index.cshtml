@model IEnumerable<AssessmentAcc.Models.Transactions>

@{
    ViewData["Title"] = "Index";
}

<h1>Transactions List</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomorUrut)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustomerId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TanggalPemesanan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TanggalPengiriman)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TermOfPayment)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JumlahNominal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StatusApproval)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomorUrut)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TanggalPemesanan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TanggalPengiriman)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TermOfPayment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JumlahNominal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StatusApproval)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
